@*@model MK_MVC.ViewModels.PeopleViewModel;
@using MK_MVC.Models*@
@model IEnumerable<Country>

@{
    ViewData["Title"] = "Index";
}

<a asp-action="Create">Add Country</a>
<br />
<table class="table">
    <thead>
        <tr>
            <th>Country Id</th>
            <th>Country</th>
        </tr>
    </thead>
    <tbody>
        @foreach ( var item in Model)
        {
            <tr>
                <td>@item.CountryId</td>
                <td>@item.CountryName</td>
            </tr>
        }
    </tbody>
</table>