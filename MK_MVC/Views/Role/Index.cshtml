@model IEnumerable<Microsoft.AspNetCore.Identity. IdentityRole>
@{
    ViewData["Title"] = "Index";
}

<h1>Roles</h1>

<a asp-action="Create">Create role</a>
<>
<a asp-action="AddUserToRole">Add user to role</a>

<table class="table">
    <thead>
        <tr>
            <th>Id</th>
            <th>Name</th>
        </tr>
    </thead>
    <tbody>
        @foreach(var item in Model)
        {
            <tr>
                <th>@item.Id</th>
                <th>@item.Name</th>
            </tr>
        }
    </tbody>
</table>
