
@{
    ViewData["Title"] = "Add Language to Person";
}

<form asp-controller="PersonLanguage" asp-action="Create" method="post">
    
    
    <div class="form-group">
    <select name="languageid" asp-items="ViewBag.Language"></select>
    </div>

     <div class="form-group">
    <select name="personid" asp-items="ViewBag.People"></select>
    </div>
    <!--   -->
  
    <input type="submit" value="Add  Language to Person" /><br>
</form>
